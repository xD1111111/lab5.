"use strict";

function array() {
  const items = [];

  function arr(index) {
    return items[index];
  }

  arr.push = function(value) {
    items.push(value);
  };

  arr.pop = function() {
    return items.pop();
  };

  return arr;
}
const arr = array();

arr.push('first');
arr.push('second');
arr.push('third');

console.log(arr(0));
console.log(arr(1));
console.log(arr(2));

console.log(arr.pop());
console.log(arr.pop());
console.log(arr.pop());

console.log(arr.pop());
